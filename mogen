#!/bin/bash
set -o errexit
set -o nounset

# mogen: generate .mo and copy it to the correct location

cd ~/kz-scripts/usr/bin
msgfmt --output-file=../share/locale/nl/LC_MESSAGES/kz.mo ../share/locale/nl/LC_MESSAGES/kz.po
sudo cp ../share/locale/nl/LC_MESSAGES/kz.mo /usr/share/locale/nl/LC_MESSAGES/kz.mo
